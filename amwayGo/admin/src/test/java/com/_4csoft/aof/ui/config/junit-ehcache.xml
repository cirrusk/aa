<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true" monitoring="autodetect"
	dynamicConfig="true">

	<diskStore path="java.io.tmpdir" />

	<defaultCache 
		maxElementsInMemory="10000" 
		eternal="false" 
		timeToIdleSeconds="120" 
		timeToLiveSeconds="120" 
		overflowToDisk="true"
		diskSpoolBufferSizeMB="30" 
		maxElementsOnDisk="10000000" 
		diskPersistent="false" 
		diskExpiryThreadIntervalSeconds="120" 
		memoryStoreEvictionPolicy="LRU"
		statistics="false" />

	<!-- Code -->
	<cache name="cacheCode" 
		maxElementsInMemory="1000" 
		eternal="false" 
		timeToIdleSeconds="1800" 
		timeToLiveSeconds="1800" 
		overflowToDisk="true" 
		diskPersistent="false"
		maxElementsOnDisk="100000" 
		memoryStoreEvictionPolicy="LRU" />

	<!-- rolegroupAccess -->
	<cache name="cacheRolegroupAccess" 
		maxElementsInMemory="1000" 
		eternal="false" 
		timeToIdleSeconds="1800" 
		timeToLiveSeconds="1800" 
		overflowToDisk="true" 
		diskPersistent="false"
		maxElementsOnDisk="100000" 
		memoryStoreEvictionPolicy="LRU" />

	<!-- RolegroupMenu -->
	<cache name="cacheRolegroupMenu" 
		maxElementsInMemory="1000" 
		eternal="false" 
		timeToIdleSeconds="1800" 
		timeToLiveSeconds="1800" 
		overflowToDisk="true" 
		diskPersistent="false"
		maxElementsOnDisk="100000" 
		memoryStoreEvictionPolicy="LRU" />

	<!-- category -->
	<cache name="cacheCategory" 
		maxElementsInMemory="1000" 
		eternal="false" 
		timeToIdleSeconds="1800" 
		timeToLiveSeconds="1800" 
		overflowToDisk="true" 
		diskPersistent="false"
		maxElementsOnDisk="100000" 
		memoryStoreEvictionPolicy="LRU" />

	<!-- Setting -->
	<cache name="cacheSetting" 
		maxElementsInMemory="1000" 
		eternal="false" 
		timeToIdleSeconds="1800" 
		timeToLiveSeconds="1800" 
		overflowToDisk="true" 
		diskPersistent="false"
		maxElementsOnDisk="100000" 
		memoryStoreEvictionPolicy="LRU" />
  
    <!-- YearTerm -->
    <cache name="cacheYearTerm" 
        maxElementsInMemory="1000" 
        eternal="false" 
        timeToIdleSeconds="1800" 
        timeToLiveSeconds="1800" 
        overflowToDisk="true" 
        diskPersistent="false"
        maxElementsOnDisk="100000" 
        memoryStoreEvictionPolicy="LRU" />

</ehcache>

